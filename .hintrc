{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/aria": [
      "default",
      {
        "aria-input-field-name": "off"
      }
    ],
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/text-alternatives": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "-ms-filter"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 127",
    "not ios_saf <= 17.5",
    "not opera <= 110",
    "not safari <= 17.5"
  ]
}